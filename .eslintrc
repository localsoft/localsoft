{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "no-extra-parens": "error",
        "prettier/prettier": "error",
        "sort-imports": [2, {
            "ignoreCase": true,
            "ignoreMemberSort": false,
            "memberSyntaxSortOrder": ["multiple", "single", "all", "none"]
        }]
    }
}
